{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "basic api based on fastify ",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "db:create": "sequelize-cli db:create",
